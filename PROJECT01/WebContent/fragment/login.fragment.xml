<core:FragmentDefinition xmlns="sap.m" xmlns:c="sap.ui.commons"
	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Dialog showHeader="false" contentWidth="25%" contentHeight="70%">
	    <customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<ToolbarSpacer/>
				<Button icon="sap-icon://decline" press="close"/>
			</Toolbar>
		</customHeader>
		<IconTabBar	class="sapUiResponsiveContentPadding" id="idIconTabBar">
			<items>
				<IconTabFilter key="keySignUp" text="Sign Up">
					<VBox>
						<HBox>
							<Label labelfor="idName" width="80px" text="Name" design="Bold" />
							<Input id="idName" />
						</HBox>
						<HBox>
							<Label width="80px" text="Mobile" design="Bold" />
							<Input id="idMobile" />
						</HBox>
						<HBox>
							<Label width="80px" text="Email" design="Bold" />
							<Input id="idEmail" />
						</HBox>
						<HBox>
							<Label width="80px" text="Password" design="Bold" />
							<Input type="Password" id="idPassword" />
						</HBox>
						<HBox>
							<Button type="Accept" text="Register" press="register" />
						</HBox>
					</VBox>
				</IconTabFilter>
				<IconTabFilter key="keySignIn" text="Sign In">
					<VBox>
						<HBox>
							<Label width="80px" text="Email" design="Bold" />
							<Input id="idSEmail" />
						</HBox>
						<HBox>
							<Label width="80px" text="Password" design="Bold" />
							<Input type="Password" id="idSPassword" />
						</HBox>
						<HBox>
							<Button type="Accept" text="Sign In" press="" />
						</HBox>
					</VBox>
				</IconTabFilter>
			</items>
		</IconTabBar>
	</Dialog>
</core:FragmentDefinition>