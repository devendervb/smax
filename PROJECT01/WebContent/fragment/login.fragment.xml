<core:FragmentDefinition xmlns="sap.m"	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<Dialog showHeader="false">
		<IconTabBar id="idIconTabBarNoIcons" expanded="{device>/isNoPhone}"	class="sapUiResponsiveContentPadding">
			<items>
				<IconTabFilter text="Sign Up">

					<l:VerticalLayout class="sapUiContentPadding"
						width="100%">

						<l:content>

							<Label text="Name" class="sapUiSmallMarginBottom" design="Bold" />

							<Input id="idName" />

							<Label text="Mobile" class="sapUiSmallMarginBottom" design="Bold" />

							<Input id="idMobile" />

							<Label text="Email" class="sapUiSmallMarginBottom" design="Bold" />

							<Input id="idEmail" />

							<Label text="Password" class="sapUiSmallMarginBottom"
								design="Bold" />

							<Input id="idPassword" />

							<Button type="Accept" text="Register" press="" />

						</l:content>

					</l:VerticalLayout>

				</IconTabFilter>


				<IconTabFilter text="Sign In">

					<l:VerticalLayout class="sapUiContentPadding"
						width="100%">

						<l:content>

							<Label text="Email" class="sapUiSmallMarginBottom" design="Bold" />

							<Input id="idSEmail" />

							<Label text="Password" class="sapUiSmallMarginBottom"
								design="Bold" />

							<Input id="idSPassword" />

							<Button type="Accept" text="Sign In" press="" />

						</l:content>

					</l:VerticalLayout>

				</IconTabFilter>

			</items>

		</IconTabBar>

	</Dialog>

</core:FragmentDefinition>